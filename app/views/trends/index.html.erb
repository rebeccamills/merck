<p id="notice"><%= notice %></p>

<h1>Merck Trendbreak Dashboard Portal</h1>

<div id="menu">
<nav>
<ul>
<li>Themis</li>
<li>Xponent</li>
<li>Truven</li>
<li>Cerner</li>
<li>GE</li>
</ul>
</nav>
</div>
<table>

  <thead>
    <tr>
      <th>Country</th>
      <th>Product</th>
      <th>Strength</th>
      <th>Marketshare</th>
      <th>Volume</th>
      <th>Dashboard</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trends.each do |trend| %>
      <tr>
        <td><%= trend.country %></td>
        <td><%= trend.product %></td>
        <td><%= trend.strength %></td>
        <td><%= trend.cp_marketshare %></td>
        <td><%= trend.cp_volume %></td>
        <td><%= trend.dashboard %></td>
       <!--  <td><%= link_to 'Show', trend %></td>
        <td><%= link_to 'Edit', edit_trend_path(trend) %></td>
        <td><%= link_to 'Destroy', trend, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<h3>Import Trends</h3>
<%= form_tag import_trends_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Upload Trends" %>
<% end %> 
